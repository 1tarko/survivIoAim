{
	"name": "survivIoZoom",
	"version": "1.0",
	"description": "survivIoZoom chrome extension",
	"homepage_url": "https://github.com/w3x731/survivIoAim/",
	"manifest_version": 2,
	"permissions": [
		"declarativeContent",
		"webRequest",
		"webRequestBlocking",
		"*://surviv.io/"
	],

	"content_security_policy":  "script-src 'self' 'unsafe-eval'; object-src 'self'",

	"web_accessible_resources": [
		"js/app.57df691c.js"
	],
		
	"background": {
		"scripts": ["background.js"],
		"persistent": true
	},
	"page_action": {
		"default_icon": {
			"16": "images/search16.png",
			"32": "images/search32.png",
			"48": "images/search48.png",
			"128": "images/search128.png"
		}
	},
	"icons": {
		"16": "images/search16.png",
		"32": "images/search32.png",
		"48": "images/search48.png",
		"128": "images/search128.png"
	}
}